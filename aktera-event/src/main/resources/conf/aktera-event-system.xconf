<keel>

	<!-- Module info -->

	<aktera.module-info id="aktera.module-info">
		<module id="${pom.artifactId}" type="service">
			<name>${pom.name}</name>
			<version>${pom.version}</version>
			<description>${pom.description}</description>
			<copyright>${aktera.copyright}</copyright>
		</module>
	</aktera.module-info>

	<!-- Startup -->

	<aktera.startup id="aktera.startup" activation="startup" logger="aktera">
		<startup id="de.iritgo.aktera.event.EventManager" bean="de.iritgo.aktera.event.EventManager" />
	</aktera.startup>

	<!-- Events -->

	<events id="events" activation="startup" logger="aktera"/>

</keel>
